{
    "name": "saeidsharafi/laravel-sms",
    "description": "notification channel for sending sms.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "sms"
    ],
    "authors": [
        {
            "name": "Saeid Sharafi",
            "email": "saeid_sharafi12@yahoo.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/notifications": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
      "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "SaeidSharafi\\LaravelSms\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SaeidSharafi\\LaravelSms\\Providers\\SmsServiceProvider"
            ],
            "aliases": {
                "Sms": "SaeidSharafi\\LaravelSms\\Facade\\Sms"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.5|^10.0"
    }
}
